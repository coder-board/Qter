
.header{
	width:100%;
	font-size:23px;
	color: #333333;
}

.header .title{
	margin:20px 16px;
}

.header .pick-header{
	width: 116px;
	height: 40px;
	border: 1px solid #EEEEEE;
	margin-left: 16px;
	display: flex;
	flex-direction: row;
	position: relative;
	border-radius: 5px;
}

.img image{
	width: 17px;
	height: 17px;
	margin:12px 8px 12px 16px;
}
.year{
     font-size:15px;
	 margin-top: 10px;
}

.arr{
	position: absolute;
	border:1px #333333 solid;
	transform: rotate(45deg);
	height: 12px;
	width: 12px;
	border-top: 0;
	border-left: 0;
	top:10px;
	right: 20px;
}

.content,.goes{
	width: 100%;
}

.content .percent{
	display: flex;
	flex-direction: row;
}
.content .rank{
    flex-grow: 1;
    text-align: center;
    border-right:1px solid #EEEEEE;
}

.content .rating{
	color: #FF6600;
	font-size: 18px;
	margin:5px 0;
}

.content .office{
	font-size:15px;
	font-weight: lighter;
}

.goes .title{
	margin:20px 16px;
	font-size: 23px;
}

.goes .percent-content{
	font-size:15px;
}
.goes .percent-content{
	display: flex;
	flex-direction: row;
	margin:10px 18px 20px 2px;
}

.goes .percent-goes{
	width: 90%;
	margin-left: 5%;
}
.goes .percent-goes .pro{
    height: 16px;
    border:1px solid #FF6600;
    border-radius: 5px;
}

.goes .percent-office{
	margin-right: 20px;
}

.goes .percent-office span{
    color: #FF6600;
} 